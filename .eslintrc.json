// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
  },
  "env": {
      "es6":     true,
      "node":    true,
      "mocha":   true
  },
  "extends": "airbnb",
  "rules": {
  
    "indent": ["error", 4],
    "max-len": ["error", { "code": 120 }],
    "no-console": "off"
  }
}